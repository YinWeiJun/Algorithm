class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        pre, n = nums[0], 1
        i = 1
        while i < len(nums) and n < (len(nums) - i):
            num = nums[i]
            if n == 0:
                pre, n = num, 1
            else:
                if num == pre:
                    n += 1
                else:
                    n -= 1
            i += 1
        return pre
