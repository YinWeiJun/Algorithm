class Solution(object):
    def isPalindrome(self, x):
        if x<0:
            return False
        y=0
        temp=x
        while temp!=0:
            y=y*10+temp%10
            temp=temp/10
        if y==x:
            return True
        else:
            return False

        """
        :type x: int
        :rtype: bool
        """
