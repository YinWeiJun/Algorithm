
class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums) < 1:
            return
        tmp = [0]*len(nums)
        for i,val in enumerate(nums):
            if i == 0:
                tmp[i] = val
            else:
                if tmp[i-1] < 0:
                    tmp[i] = val
                else:
                    tmp[i] = tmp[i-1]+val
        mx = max(tmp)
        return mx
