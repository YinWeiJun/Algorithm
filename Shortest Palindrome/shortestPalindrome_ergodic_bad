class Solution(object):
    def shortestPalindrome(self, s):
        """
        :type s: str
        :rtype: str
        """
        if not s or s == s[::-1]:
            return s
        i = -1
        while i > -1 * len(s):
            ss = s[i:][::-1] + s
            if ss == ss[::-1]:
                return ss
            i -= 1
        
