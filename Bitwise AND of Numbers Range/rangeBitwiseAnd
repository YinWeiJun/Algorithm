class Solution(object):
    def rangeBitwiseAnd(self, m, n):
        """
        :type m: int
        :type n: int
        :rtype: int
        """
        while (m<n): n &= n-1
        return n

        # i = 0
        # while(n!=m):
        #     n =>> 1
        #     m =>> 1
        #     i += 1
        # return n<<i
